{
    "name": "operation-hardcode/php-rpc-server",
    "description": "JSON RPC server implementation for PHP.",
    "type": "library",
    "keywords": [
        "php",
        "rpc",
        "json-rpc"
    ],
    "require": {
        "php": "^8.1",
        "ext-json": "*",
        "psr/log": "^3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "vimeo/psalm": "^4.15"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "OperationHardcode\\PhpRpcServer\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "OperationHardcode\\PhpRpcServer\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "kafkiansky",
            "email": "vadimzanfir@gmail.com"
        }
    ],
    "minimum-stability": "stable"
}
